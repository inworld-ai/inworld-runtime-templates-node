{
  "compilerOptions": {
    "baseUrl": "src",
    "declaration": true,
    "module": "Node16",
    "moduleResolution": "node16",
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "es6",
    "types": ["node", "jest"],
    "esModuleInterop": true,
    "isolatedModules": true
  }
}
