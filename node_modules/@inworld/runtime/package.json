{
  "name": "@inworld/runtime",
  "version": "0.9.0-rc.17",
  "license": "SEE LICENSE IN LICENSE.md and LICENSE-CPP-BINARIES.md",
  "exports": {
    ".": "./build/index.js",
    "./common": "./build/common/export.js",
    "./graph": "./build/graph/export.js",
    "./graph/nodes": "./build/graph/dsl/nodes/builtin/export.js",
    "./graph/components": "./build/graph/dsl/components/export.js",
    "./core/text_processing": "./build/core/text_processing/export.js",
    "./core": "./build/core/export.js",
    "./primitives/embedder": "./build/primitives/embeddings/index.js",
    "./primitives/embeddings": "./build/primitives/embeddings/index.js",
    "./primitives/knowledge": "./build/primitives/knowledge/index.js",
    "./primitives/llm": "./build/primitives/llm/index.js",
    "./primitives/ner": "./build/primitives/nlu/index.js",
    "./primitives/nlu": "./build/primitives/nlu/index.js",
    "./primitives/stt": "./build/primitives/speech/index.js",
    "./primitives/tts": "./build/primitives/speech/index.js",
    "./primitives/vad": "./build/primitives/speech/index.js",
    "./primitives/speech": "./build/primitives/speech/index.js",
    "./primitives/mcp": "./build/primitives/mcp/index.js",
    "./primitives/platform": "./build/primitives/platform/index.js",
    "./telemetry": "./build/telemetry/export.js"
  },
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "inworld",
    "ai",
    "asr",
    "character",
    "npc",
    "speech recognition",
    "text to speech",
    "tts",
    "voice"
  ],
  "author": {
    "name": "Inworld AI",
    "url": "https://www.inworld.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inworld-ai/inworld-framework-nodejs.git"
  },
  "scripts": {
    "binaries:download": "node ./scripts/download-unzip-binaries.cjs",
    "postinstall": "npm run binaries:unzip",
    "binaries:unzip": "node ./scripts/binaries-unzip.cjs",
    "generate:zod": "ts-node ./scripts/zod/generate-zod-schemas.ts",
    "generate:dsl": "json2ts src/graph/graph-schema-v1.x.json -o src/graph/dsl/graph_config_schema.ts --unreachableDefinitions=true",
    "generate:proto-bindings": "ts-node ./scripts/proto/generate-pb-bindings.ts",
    "build": "tsc -p .",
    "release:pack": "yarn build && node ./scripts/pack.cjs",
    "lint": "eslint \"{src,templates,__tests__,sandbox,benchmarks}/**/*.{js,ts,tsx,jsx}\" --fix",
    "lint:check": "eslint \"{src,templates,__tests__,sandbox,benchmarks}/**/*.{js,ts,tsx,jsx}\"",
    "prettier": "prettier --write \"{src,templates,__tests__,scripts}/**/*.{ts,tsx,jsx}\" --ignore-path .gitignore",
    "prettier:check": "prettier --check \"{src,templates,__tests__,benchmarks}/**/*.{js,ts,tsx,jsx}\" --ignore-path .gitignore",
    "test": "jest __tests__/package/* --config=jest.package.config.json --runInBand --no-cache --detectOpenHandles --forceExit",
    "templates:run": "jest __tests__/templates/* --config=jest.templates.config.json --runInBand --no-cache --verbose",
    "package:templates:run": "jest __tests__/templates/* --config=jest.package.templates.config.json --runInBand --no-cache --verbose",
    "test:debug": "node --inspect-brk jest __tests__/* --no-cache --runInBand",
    "test:coverage": "yarn test --coverage",
    "circular-deps": "madge --circular --extensions ts src",
    "prepare": "husky",
    "docs:generate-html": "typedoc --readme none --entryPointStrategy expand --out docs_html --disableSources --excludeInternal --excludeNotDocumented ./src",
    "docs:generate-md": "typedoc --plugin typedoc-plugin-markdown --readme none --entryPointStrategy expand --out docs_md --disableSources --excludeInternal --excludeNotDocumented ./src",
    "docs:generate-mdx": "python3 ./scripts/html_to_mdx.py --output docs_mdx docs_html",
    "docs:generate": "yarn docs:generate-html && yarn docs:generate-mdx",
    "type-check": "tsc --noEmit --project . && tsc --noEmit --project __tests__ && tsc --noEmit --project templates/ts/tsconfig.json",
    "validate:bindings": "node scripts/validate-bindings/find-unused-bindings.js"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^7.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "allure-jest": "^3.2.2",
    "allure-js-commons": "^3.2.2",
    "archiver": "^7.0.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "json-schema-to-typescript": "^15.0.3",
    "madge": "^8.0.0",
    "node-gyp": "^11.2.0",
    "prettier": "^3.5.3",
    "quicktype": "^23.2.6",
    "release-it": "^19.2.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "^5.8.2",
    "wav-decoder": "^1.3.0",
    "yargs": "^17.7.2",
    "zod": "^4.1.12"
  },
  "dependencies": {
    "@types/lodash.snakecase": "^4.1.9",
    "@types/protobufjs": "^6.0.0",
    "@zod/core": "^0.11.6",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "assemblyai": "^4.17.0",
    "chalk": "^4.1.2",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "groq-sdk": "^0.33.0",
    "lodash.snakecase": "^4.1.1",
    "node-record-lpcm16": "^1.0.1",
    "protobufjs": "^7.5.4",
    "uuid": "^11.1.0",
    "zod-to-json-schema": "^3.25.0"
  }
}
